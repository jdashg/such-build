# Build Firefox for Android:
ac_add_options --enable-application=mobile/android
ac_add_options --target=arm-linux-androideabi

# With the following java and javac:
ac_add_options --with-java-bin-path="/Library/Java/Home/bin"

# With the following Android SDK and NDK:
ac_add_options --with-android-sdk="/Users/jgilbert/.mozbuild/android-sdk-macosx"
ac_add_options --with-android-ndk="/Users/jgilbert/.mozbuild/android-ndk-r17b"

export CC=
export CXX=
